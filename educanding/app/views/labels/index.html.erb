<div class:"container">
  <h1>Etiquetas</h1>
  <table class:"table">
    <tr><th>Nombre de etiqueta</th></tr>
    <% @etiquetas.each do |e| %>
      <tr>
        <td><%= e.nombre %></td>
      </tr>
    <% end %>
  </table>
  <%= link_to "Crear Etiqueta", labels_new_path, class: 'btn btn-info btn-sm' %></br>
  <%= link_to "volver", root_path %>
</div>
