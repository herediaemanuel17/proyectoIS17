<!DOCTYPE html>
<html>
  <head>
    <title>Educanding</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      <%= link_to 'Cerrar Sesion', destroy_user_session_path, method: :delete %>
    <% else %>
      <%= link_to 'Iniciar Sesion', new_user_session_path , class: 'btn btn-info btn-sm'%>
      <!--%= link_to 'Registrarce', new_user_registration , class: 'btn btn-info btn-sm'%-->

    <% end %>
    <%= yield %>
  </body>
</html>
